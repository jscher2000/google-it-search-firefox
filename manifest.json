{
	"name": "Google Italia (google.it) Search",
	"description": "Add google.it to your search engines (including search suggestions from google.it).",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "google-it-search@jeffersonscher.com",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Jefferson \"jscher2000\" Scher",
		"url": "https://github.com/jscher2000/"
	},
	"icons": {
		"32": "favicon_it.png"
	},
	"web_accessible_resources": [
		"favicon_it.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Google Italia",
			"search_url": "https://www.google.it/search?q={searchTerms}&hl=it",
			"suggest_url": "https://www.google.it/complete/search?client=firefox&hl=it&q={searchTerms}"
		}
	}
}
